{
  "manifest_version": 2,
  "name": "Focused - Website Blocker",
  "homepage_url": "https://www.github.com/sahilkhurana19/focused-extension",
  "description": "A free and open source website blocker. Now block those distracting websites and smash your goals!",
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icons/blocked_16x16.png",
    "32": "icons/blocked_32x32.png",
    "48": "icons/blocked_64x64.png",
    "128": "icons/blocked_128x128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/blocked_16x16.png",
      "32": "icons/blocked_32x32.png",
      "48": "icons/blocked_64x64.png",
      "128": "icons/blocked_128x128.png"
    }
  },
  "options_page": "options.html"
}
